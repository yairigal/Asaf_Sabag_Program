<Page x:Class="WpfAArticleAnalysis.Pages.NormalizationPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:WpfAArticleAnalysis.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="453" d:DesignWidth="752"
      Title="NormalizationPage">

    <Grid>
        <GroupBox x:Name="groupBox" Header="Normalization" HorizontalAlignment="Left" Margin="35,32,0,0" VerticalAlignment="Top" Height="49" Width="420">
            <DockPanel>
                <CheckBox x:Name="PunRB" Content="RadioButton" Margin="0,5"/>
                <CheckBox x:Name="HTMLRB" Content="RadioButton" Height="17"/>
                <ComboBox x:Name="LettersCB" Width="212" Margin="0,0,0,5" />
            </DockPanel>
        </GroupBox>
    </Grid>
</Page>
